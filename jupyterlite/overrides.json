{
  "@jupyterlab/apputils-extension:themes": {
    "theme": "JupyterLab Night",
    "theme-scrollbars": true,
    "overrides": {
      "code-font-famiy": "Source Code Pro",
      "content-font-family": "Raleway"
    }
  },
  "@jupyterlab/notebook-extension:tracker": {
		"codeCellConfig": {
			"lineNumbers": true
		},
		"markdownCellConfig": {
			"lineNumbers": true
		},
		"rawCellConfig": {
			"lineNumbers": true
		},
    "recordTiming": true
	},
  "@retrolab/application-extension:top": {
    "visible": false
  }
}
